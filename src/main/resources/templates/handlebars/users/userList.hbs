
{{#partial "content"}}

    <h1>USERS</h1>

    {{#if users}}
        <div>
            <ul>
                {{#users}}
                    <li><a href="users/{{id}}">{{id}} - {{.}}</a></li>
                {{/users}}
            </ul>
        </div>

    {{else}}

        <div>No users to show!</div>

    {{/if}}

{{/partial}}

{{> base}}